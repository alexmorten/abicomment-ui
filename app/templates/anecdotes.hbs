<div class="quotes">
  <h3>Alle Anekdoten</h3>
  <ul class="quote-listing">

  {{#each model as |anecdote|}}
  <hr class="border">
  <li>

     <h4 class="quoted">  {{#link-to 'courses.course' anecdote.course.id}} {{anecdote.course.name}} bei {{anecdote.course.teacher}} {{/link-to}}</h4>

        <p class="quote-text">
          {{anecdote.text}}
        </p>

      </li>
  {{/each}}
  {{#if notShowingAll}}
    <button {{action "showMore"}}>Mehr laden</button>
  {{/if}}
  </ul>

    {{outlet}}
</div>

 {{liquid-outlet}}
